<script type="text/javascript">
    $(document).ready(function() {
        function cookieController() {
            // Load Google Analytics to header
            $.cookiesDirective.loadScript({
                uri:'google.js',
                appendTo: 'header'
            });
 
            // Load My Stats to body (default)
            $.cookiesDirective.loadScript({
                uri:'mystats.js'
            });
        }  
 
