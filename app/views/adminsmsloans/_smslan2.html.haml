        //%li.bold-text= @smstime_visible_info_for_user2
        //%li.bold-text= @smstime_visible_info_for_user


.row-fluid
  .span2
  .span8
    %table#table_id2#table30d.ex1.desktop
      .hulk-box#table_id2.desktop.negative-topbuffer-desktop 30 dagar
      %tbody.smsloanoverview
        - @plucker30.take(4).each do |bank, min_loanamount, max_loanamount, plucker, plucker2, max_amount_new_borrower, invoice_fee, application_fee, mobile_friendly_site, min_age, low_credit_score, cost_free_loan, cost_free_loan_amount, new_borrower_5K, new_borrower_10K, no_uc, skef_member, bankid, open_on_saturdays, open_on_sundays, weblink|
          %tr.desktop

            %td.desktop
              .lock1= bank.to_s.html_safe

            - if bank.to_s == '<img src="leasyminilan_image.png" alt="Leasy minilån" style="height:35px">'
              %td
                %p Erbjuder personlig ränta
            - elsif bank.to_s == '<img src="ferratum_image.png" alt="Ferratum" style="height:40px">'
              %td
                %p Nya kunder kan låna upp till 5000 kr i 30 dagar utan ränta.
            - elsif bank.to_s == '<img src="everydayplus_image.png" alt="Everyday+" style="height:30px">'
              %td
                %p Om du inte kan betala tillbaka på förfallodagen kan du betala 1/5 av fakturasumman, minst 200 kr om fakturasumman är 1000 kr eller mindre.
            - elsif bank.to_s == '<img src="riscium_image.png" alt="Riscium" style="height:30px">'
              %td
                %p Nya kunder kan låna upp till 4000 kr genom att endast betala uppläggningsavgift.
            - elsif bank.to_s == '<img src="vivus_image.png" alt="Vivus" style="height:30px">'
              %td
                %p Första gången kan du låna 50 kr - 6000 kr räntefritt i 30 dagar.
            - elsif bank.to_s == '<img src="extrakredit_image.png" alt="Extrakredit" style="height:34px">'
              %td
                %p 
            - elsif bank.to_s == '<img src="onea_image.png" alt="Onea" style="height:30px">'
              %td.lock
                %p Du kan flytta fram förfallodagen hur många gånger som helst då du betalar förlängningsavgiften.
            - elsif bank.to_s == '<img src="smspengar_image.png" alt="Smspengar" style="height:30px">'
              %td
                %p Upp till 64% rabatt och du kan förlänga lånen mot en ränteavgift.
            - else
              %td
                %p

            %td.desktop
              %p.lock2= excerpt(min_age, 'år', radius: 3, omission: '')
              %p.lock2= no_uc[/"(.*?)"/, 1]
              - @credit_score = low_credit_score.slice!(/(?:Ja|JA|Nej|NEJ)/)
              - @credit_score2 = @credit_score.to_s
              - if @credit_score2 == "NEJ"
                %p.lock2= "Inga betalningsanmärkningar"
              - elsif @credit_score2 == "JA"
                %p.lock2= ""
              - elsif @credit_score2 == "Ja"
                %p.lock2= ""
              - elsif @credit_score2 == "Nej"
                %p.lock2= "Inga betalningsanmärkningar"


            %td.desktop
              .special-links= weblink.to_s.html_safe



