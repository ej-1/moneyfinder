:javascript


  //scroll to top
  $('.scrollToTop').click(function(){
    $('html, body').animate({scrollTop : 0},800);
    return false;
  });

  // Keeps hidden the things that should be hidden by the checked checkboxes
  // had to move this code snippet from smslan.erb.js otherwise rows are counted and then hidden, so all rows are hidden and not ignore those that will be hidden. This code snippet has to be executed before the one below, that is why I moved it here.
  $('input:checked').each(function() {
    $('td:contains("' + $(this).val() + '")').parentsUntil(".smsloanoverview").hide();
  });
  // Counts number of loans shown and informs user after they have changed the dropdown in the smsloaninterface. Didn't work so good to put in application.js or smslan.js.erb
  rowCount = $('.anti_border_spacing tr:visible').size();
  $('.bold-text.nr_of_loans').text(rowCount + " st lån visas");

  //When cliking button to show more loans, loangroups with no loans are kept hidden.



  if ($(window).width() < 768) {
    //MOBILE

    $( ".showmoreloans_button1" ).click(function() {
        $( ".mobile.hidden1" ).show();
        $( ".hidden-button1" ).hide();
        $( ".hidden-button2" ).show();
    });


    $( ".showmoreloans_button2" ).click(function() {
        $( ".mobile.hidden2" ).show();

        var t14d = ($('#table14d tr:visible').size());
        var t21d = ($('#table21d tr:visible').size());
        var t30d = ($('#table30d tr:visible').size());
        var t60d = ($('#table60d tr:visible').size());
        var t90d = ($('#table90d tr:visible').size());
        var t1y = ($('#table1y tr:visible').size());

        $('.mobile.hidden2').each(function() {

            if (t14d === 0) {
              $('.table14d').hide();
            }

            if (t21d === 0) {
              $('.table21d').hide();
            }

            if (t14d === 0) {
              $('.table14d').hide();
            }

            if (t30d === 0) {
              $('.table30d').hide();
            }

            if (t60d === 0) {
              $('.table60d').hide();
            }

            if (t14d === 0) {
              $('.table14d').hide();
            }

            if (t90d === 0) {
              $('.table90d').hide();
            }

            if (t1y === 0) {
              $('.table1y').hide();
            }

            $( ".hidden-button2" ).hide();
        });
    });



  } else {
    //DESKTOP
    $( ".showmoreloans_button1" ).click(function() {
        $( ".desktop.hidden1" ).show();
        $( ".hidden-button1" ).hide();
        $( ".hidden-button2" ).show();
    });


    $( ".showmoreloans_button2" ).click(function() {
        $( ".desktop.hidden2" ).show();

        var t14d = ($('#table14d tr:visible').size());
        var t21d = ($('#table21d tr:visible').size());
        var t30d = ($('#table30d tr:visible').size());
        var t60d = ($('#table60d tr:visible').size());
        var t90d = ($('#table90d tr:visible').size());
        var t1y = ($('#table1y tr:visible').size());

        $('.desktop.hidden2').each(function() {

            if (t14d === 0) {
              $('.table14d').hide();
            }

            if (t21d === 0) {
              $('.table21d').hide();
            }

            if (t14d === 0) {
              $('.table14d').hide();
            }

            if (t30d === 0) {
              $('.table30d').hide();
            }

            if (t60d === 0) {
              $('.table60d').hide();
            }

            if (t14d === 0) {
              $('.table14d').hide();
            }

            if (t90d === 0) {
              $('.table90d').hide();
            }

            if (t1y === 0) {
              $('.table1y').hide();
            }

            $( ".hidden-button2" ).hide();
        });
    });

  }




.row
  .scrollToTop Scrolla till toppen


.row-fluid.row-padding-fix
  .span2
  .span8
    .center_element2.desktop
      %ul#smsloan_options_chosen.desktop


