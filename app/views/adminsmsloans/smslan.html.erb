<h1>SMS lån</h1>

<table>
  <thead>
    <tr>
      <th>Låneföretag</th>
      <th>Minsta lånesumma</th>
      <th>Högsta lånesumma</th>
      <th>Mobilvänlig site</th>
      <th>Kreditnotering</th>
      <th>Minsta ålder</th>
      <th>Kostnadsfria lån</th>
      <th>5000 kr lån första gången</th>
      <th>10 000 kr lån första gången</th>
      <th>Ingen UC</th>
      <th>SKEF medlem</th>
      <th>BankID</th>
      <th>Lördagsöppet</th>
      <th>Ansök här</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @smsloans.each do |smsloan| %>
      <tr>
        <td><%= smsloan.bank %></td>
        <td><%= smsloan.min_loanamoun %></td>
        <td><%= smsloan.max_loanamoun %></td>
        <td><%= smsloan.mobile_friendly_site %></td>
        <td><%= smsloan.smsloan_payment_note %></td>
        <td><%= smsloan.smsloan_min_age %></td>
        <td><%= smsloan.cost_free_loan %></td>
        <td><%= smsloan.new_borrower_5K %></td>
        <td><%= smsloan.new_borrower_10K %></td>
        <td><%= smsloan.no_uc %></td>
        <td><%= smsloan.skef_member %></td>
        <td><%= smsloan.bankid %></td>
        <td><%= smsloan.open_on_saturdays %></td>
        <td><%= smsloan.weblink %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
